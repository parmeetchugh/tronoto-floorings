<script type="text/javascript" id="getLocale">
   document.addEventListener('DOMContentLoaded', function () {
   // console.log(document.cookie);
    const savedCity = getCookie("city");
    const savedZip = getCookie("zipcode");
    const savedData = sessionStorage.getItem("locationData");
    const cityDisplay = document.getElementById('closest-city');
    const zipDisplay = document.getElementById('closest-zip');
   // console.log(sessionStorage);
   // console.log("SavedCity: ", savedCity);
  if(savedCity == null && savedZip == null){
    if (navigator.geolocation) {
      navigator.geolocation.getCurrentPosition(successCallback, errorCallback);
    } else {
      // Handle geolocation not being supported
    }
  } else {
    cityDisplay.textContent = `${savedCity}`;
    zipDisplay.textContent = `${savedZip}`;
    if(savedData) {
      const parsedData = JSON.parse(savedData);      
    }
  }
  });

  
function successCallback(position) {
  const latitude = position.coords.latitude;
  const longitude = position.coords.longitude;
  const cityDisplay = document.getElementById('closest-city');
  const zipDisplay = document.getElementById('closest-zip');
  const url = `https://api.storepoint.co/v1/16723da996a0c8/locations?lat=${latitude}&long=${longitude}&radius=100`;
  fetch(url)
  .then(response => {
    if (!response.ok) {
      throw new Error(`HTTP error! status: ${response.status}`);
    }
    return response.json();
  })
  .then(data => {
   // console.log("Data::: ",data);
    sessionStorage.setItem("locationData", JSON.stringify(data));
    const addr = data.results.locations[0].streetaddress;

    const match = addr.match(/,\s*([^,]+),\s*(\w{2})\s*(\d{5})/);
   // console.log("Match: ",match);
    if(match) {
      const city = match[1];
      const state = match[2];
      const zipCode = match[3];
      setLocationCookie("city", city, 1);
      setLocationCookie("state",state,1);
      setLocationCookie("zipcode",zipCode,1);
      cityDisplay.textContent = `${city}`;
      zipDisplay.textContent = `${zipCode}`;
    }
  })
  .catch(error => {
    console.log("Error fetching data: ", error);
  });
}

function setLocationCookie(name,value,days) {
  const date = new Date();
  date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
  const expires = `expires=${date.toUTCString()}`;
  const cookieString = `${name}=${value}; ${expires}; path=/; SameSite=Lax; Secure`;
  document.cookie = cookieString;
}

function getCookie(name) {
  const nameEQ = `${name}=`;
  const cookies = document.cookie.split(';');
  for(let i = 0; i < cookies.length; i++) {
    let cookie = cookies[i].trim();
    if(cookie.startsWith(nameEQ)) {
      return cookie.substring(nameEQ.length, cookie.length);
    }
  }
  return null;
}

function errorCallback(error) {
  // Handle errors
  console.error("Error getting geolocation:", error);
}


